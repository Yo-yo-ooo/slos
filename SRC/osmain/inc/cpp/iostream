#ifndef IOSTREAM
#define IOSTREAM

#include <inc/cpp/iosfwd>
#include <inc/malloc.h>

namespace std{

extern istream cin;
extern ostream cout;
extern ostream cerr;
extern ostream clog;
    
}

#ifdef CPP_NEW_INSERT
void* operator new(size_t Size) {return malloc(Size);}
void* operator new[](size_t Size) {return malloc(Size);}
#endif

#endif